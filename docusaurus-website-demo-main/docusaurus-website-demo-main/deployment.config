# Deployment Configuration for Agentic AI Documentation

# Environment variables for deployment
ENVIRONMENT=production
SITE_URL=https://agentic-ai-documentation.example.com
BASE_URL=/

# Build settings
BUILD_DIR=build
OUTPUT_FORMAT=static

# Docusaurus specific settings
DOCUSAURUS_VERSION=3.9.2
USE_CLIENT_MIGRATION=true

# Analytics
ANALYTICS_ID=G-XXXXXXXXXX

# Search configuration
ALGOLIA_APP_ID=your_app_id
ALGOLIA_SEARCH_API_KEY=your_search_api_key
ALGOLIA_INDEX_NAME=agentic-ai-docs

# Deployment settings
DEPLOY_BRANCH=gh-pages
DEPLOY_PRIME_BRANCH=main

# Performance settings
ENABLE_MINIFICATION=true
ENABLE_BUNDLING=true
COMPRESS_ASSETS=true

# Feature flags
ENABLE_PWA=false
ENABLE_DEBUG=false